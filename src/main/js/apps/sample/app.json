{
    "load": {
        "bundleLocations": ["localbundles", "bundles"],
        "allowedBundles": [
            "system",
            "splashscreen",
            "map",
            "themes",
            "templates",
            "templatelayout",
            "windowmanager",
            "toolset",
            "toolrules",
            "basemaptoggler",
            "banner",
            "content_office_locations"
        ]
    },
    "bundles": {
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "image": "https://www.conterra.de/~/media/conterra/images/site_logos/conterra-logo.ashx",
                "imagePosition": "above",
                "link": "http://developernetwork.conterra.de"
            }
        },
        "map": {
            "MapState": {
                "initialExtent": {
                    "xmin": -1100000,
                    "ymin": 4000000,
                    "xmax": 3500000,
                    "ymax": 8800000,
                    "spatialReference": {
                        "wkid": 3857
                    }
                }
            },
            "MapModelFactory": {
                "_configData": {
                    "maps": [
                        {
                            "id": "default",
                            "glass_pane": [],
                            "operationalLayer": [],
                            "baseLayer": [
                                {
                                    "id": "worldimage",
                                    "title": "World Topo",
                                    "service": "topo",
                                    "layers": ["*"],
                                    "enabled": true
                                }
                            ]
                        }
                    ]
                }
            },
            "MappingResourceRegistryFactory": {
                "_knownServices": {
                    "services": [
                        {
                            "id": "topo",
                            "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer",
                            "type": "AGS_TILED"
                        }
                    ]
                }
            }
        }
    }
}